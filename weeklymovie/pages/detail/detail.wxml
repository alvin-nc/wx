<view class='container'>
  <image class='detail-image' src='{{movie.images.small}}'></image>
  <text style='font-weight:bold; font-size: 50rpx; '>{{movie.title}}</text>
  <text>豆瓣想看： {{movie.wish_count}}</text>
  <text>豆瓣看过：{{movie.collect_count}}</text>
  <text>豆瓣评分：{{movie.rating.average}}</text>
  <text>豆瓣简介：{{movie.summary}}</text>
</view>